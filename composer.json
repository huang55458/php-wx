{
    "name": "topthink/think",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "https://www.thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        },
        {
            "name": "yunwuxin",
            "email": "448901948@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-pcntl": "*",
        "topthink/framework": "^8.0",
        "topthink/think-orm": "^3.0",
        "topthink/think-filesystem": "^2.0",
        "topthink/think-helper": "^3.1",
        "gaoming13/wechat-php-sdk": "1.*",
        "topthink/think-migration": "^3.1",
        "topthink/think-worker": "^4.0",
        "topthink/think-swoole": "^4.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpmailer/phpmailer": "^6.9",
        "rmccue/requests": "^2.0",
        "symfony/console": "^7.0",
        "topthink/think-view": "^2.0",
        "topthink/think-annotation": "^2.0",
        "orhanerday/open-ai": "^5.1",
        "elasticsearch/elasticsearch": "^8.13",
        "monolog/monolog": "^3.6",
        "intervention/image": "^3.6"
    },
    "require-dev": {
        "symfony/var-dumper": ">=4.2",
        "topthink/think-trace": "^1.0",
        "phpunit/phpunit": "^10.5",
        "swoole/ide-helper": "^4.8",
        "friendsofphp/php-cs-fixer": "^3.51",
        "icomefromthenet/reverse-regex": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    }
}
